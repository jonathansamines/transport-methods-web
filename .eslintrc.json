{
  "extends": [
    "airbnb/base",
    "airbnb/rules/react"
  ],
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "env": {
    "browser": true
  },
  "rules": {
    "react/prefer-stateless-function": 0,
    "react/jsx-closing-bracket-location": 0,
    "react/sort-comp": 0,
    "arrow-parens": 0,
    "arrow-body-style": 0,
    "no-param-reassign": 0,
    "no-loop-func": 0,
    "no-continue": 0
  }
}
